// Online C compiler to run C program online
#include <stdio.h>
#include<stdlib.h>
int main () {
    int n;
    printf ("enter size:");      
    scanf ("%d",&n);
    int *arr = (int*) malloc(n*sizeof(int));
    for (int i=0;i<10;i++){
        printf ("enter element%d:",i+1);
        scanf ("%d",&arr[i]);
    }
    printf ("array elements:\n") ;
    for (int i=0;i<10;i++){
    printf("%d",arr[i]);
    }
    free(arr);
    return 0;
}
